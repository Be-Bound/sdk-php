{
  "name": "bebound/sdk-php",
  "description": "Simple library to implement Be-Bound on a PHP 7 backend",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Lionel Brianto",
      "email": "lbrianto@be-bound.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.1 || ^7.2",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "pds/skeleton": "^1.0",
    "phpstan/phpstan": "^0.9.2",
    "phpunit/phpunit": "^7.0",
    "roave/security-advisories": "@dev",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "BeBound\\SDK\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  }
}
